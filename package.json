{
  "name": "breakable-toy",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "user-microservice": "start nx serve user",
    "user:g:api-client": "openapi-generator generate -i tools/schematics/user.openapi.json -g typescript-angular -o libs/shared/data-access/user-api-client/src/lib",
    "auth-microservice": "start nx serve auth",
    "auth:g:api-client": "openapi-generator generate -i tools/schematics/auth.openapi.json -g typescript-angular -o libs/shared/data-access/auth-api-client/src/lib",
    "todo:g:api-client": "openapi-generator generate -i tools/schematics/todo.openapi.json -g typescript-angular -o libs/todo/data-access/todo-api-client/src/lib",
    "todo:cp:frontend": "cp -r ./apps/todo/* ./docker/todo-app/todo/src/apps/todo && cp -r angular.json package*.json tsconfig.json ./docker/todo-app/todo/src && cp -r ./libs/todo/data-access/todo-api-client/* ./docker/todo-app/todo/src/libs/todo/data-access/todo-api-client",
    "todo:cp:backend": "cp -r ./apps/todo-api/* ./docker/todo-app/todo-api/src/apps/todo-api && cp -r ./libs/shared/data-access/mongo-storage/*  ./docker/todo-app/todo-api/src/libs/shared/data-access/mongo-storage && cp -r angular.json nx.json package*.json tsconfig.json ./docker/todo-app/todo-api/src",
    "todo:docker:prod": "npm run todo:cp:frontend && npm run todo:cp:backend && cd docker/todo-app && docker-compose up --build",
    "todo-app:g:api-clients": "npm run auth:g:api-client && npm run user:g:api-client && npm run todo:g:api-client",
    "todo-app:serve": "start nx serve todo && start nx serve todo-api && start nx serve user && start nx serve auth"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/microservices": "^7.1.3",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^6.8.3",
    "@nrwl/angular": "9.4.0",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.5.6",
    "bcrypt": "^5.0.0",
    "core-js": "^2.5.4",
    "jwt-decode": "^2.2.0",
    "mongodb": "^3.5.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "uuid": "^8.1.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/swagger": "^4.5.8",
    "@nestjs/testing": "^6.11.11",
    "@nrwl/cypress": "9.4.0",
    "@nrwl/jest": "9.4.0",
    "@nrwl/nest": "9.4.0",
    "@nrwl/node": "9.4.0",
    "@nrwl/workspace": "9.4.0",
    "@openapitools/openapi-generator-cli": "^1.0.13-4.3.1",
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "25.1.4",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "~8.9.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "codelyzer": "~5.0.1",
    "cypress": "^4.1.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "prettier": "2.0.4",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3"
  }
}
